{
    "name": "jate",
    "version": "1.0.0",
    "description": "Just Another Text Editor",
    "main": "server/server.js",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server/server.js --ignore client",
        "client": "webpack-dev-server --config client/webpack.config.js --open",
        "build": "webpack --mode production --config client/webpack.config.js --stats-children",
        "postinstall": "npm run build",
        "heroku-postbuild": "npm install --only=dev && npm run build"
      },
      
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "express": "^4.17.1",
      "idb": "^5.0.4"
    },
    "devDependencies": {
      "concurrently": "^5.3.0",
      "html-webpack-plugin": "^4.5.0",
      "webpack": "^5.4.0",
      "webpack-cli": "^4.1.0",
      "webpack-dev-server": "^3.11.0",
      "webpack-pwa-manifest": "^4.2.0",
      "workbox-webpack-plugin": "^7.0.0",
      "babel-loader": "^8.1.0",
      "css-loader": "^5.0.0",
      "style-loader": "^2.0.0",
      "nodemon": "^2.0.4"
    }
  }
  