{
  "name": "jate",
  "version": "1.0.0",
  "description": "Just Another Text Editor",
  "main": "server/server.js",
  "scripts": {
    "serve": "serve -s dist",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js --ignore client",
    "client": "webpack-dev-server --config client/webpack.config.js --open",
    "build": "webpack --mode production --config client/webpack.config.js --stats-children",
    "postinstall": "npm run build",
    "heroku-postbuild": "npm install --only=dev && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "idb": "^5.0.4"
  },
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
